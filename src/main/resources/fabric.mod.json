{
	"schemaVersion": 1,
	"name": "${mod_name}",
	"id": "${mod_id}",
	"version": "${version}",
	"icon": "assets/${mod_id}/textures/logo.png",
	"authors": [
		"Hugman"
	],
	"contact": {
		"sources": "https://github.com/DawnTeamMC/${github_name}",
		"issues": "https://github.com/DawnTeamMC/${github_name}/issues"
	},
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.hugman.dawn.Dawn"
		],
		"client": [
			"com.hugman.dawn.DawnClient"
		],
		"modmenu": [
			"com.hugman.dawn.config.DawnModMenuIntegration"
		]
	},
	"mixins": [
		"dawn_api.mixins.json",
		"dawn_mod.mixins.json"
	],
	"depends": {
		"minecraft": ">=1.16.2",
		"fabric": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/8ksTVJu",
				"modmenu.dawn.curseforge": "https://www.curseforge.com/minecraft/mc-mods/${curseforge_slug}",
				"modmenu.dawn.modrinth": "https://modrinth.com/mod/${modrinth_slug}"
			},
			"badges": [
				"library"
			]
		},
		"modupdater": {
			"strategy": "curseforge",
			"projectID": 399309
		}
	},
	"license": "MIT"
}
